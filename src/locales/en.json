  "help": {
    "title": "Help",
    "sections": {
      "whatIs": {
        "title": "What is Mytaskly?",
        "description": "Mytaskly is your personal app for managing daily activities. Organize your tasks, create custom categories and keep everything under control with a simple and intuitive interface."
      },
      "features": {
        "title": "Main features",
        "taskManagement": {
          "title": "Task Management",
          "description": "Create, edit and delete your activities with ease"
        },
        "categories": {
          "title": "Categories",
          "description": "Organize your tasks in custom categories"
        },
        "notes": {
          "title": "Notes",
          "description": "Add notes and memos to never forget anything"
        },
        "aiAssistant": {
          "title": "AI Assistant",
          "description": "Chat with the AI assistant to better organize your activities"
        },
        "notifications": {
          "title": "Notifications",
          "description": "Receive reminders to never miss a deadline"
        }
      },
      "howToUse": {
        "title": "How to use the app",
        "step1": {
          "title": "Start from Home",
          "description": "The main screen shows you an overview of your activities"
        },
        "step2": {
          "title": "Create your categories",
          "description": "Go to the Categories section to organize your tasks"
        },
        "step3": {
          "title": "Add your tasks",
          "description": "Create new activities and assign them to appropriate categories"
        },
        "step4": {
          "title": "Use the AI Assistant",
          "description": "Chat with the assistant to get suggestions and better organize your time"
        },
        "step5": {
          "title": "View your statistics",
          "description": "Access the Statistics section to monitor your progress, view charts and analyze your performance"
        }
      },
      "tips": {
        "title": "Tips",
        "tip1": "Set reminders for your important tasks to stay on track",
        "tip2": "Use categories to organize your tasks by project or area of life",
        "tip3": "Review your progress regularly to improve your productivity"
      }
    }
  },
