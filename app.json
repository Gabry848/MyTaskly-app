{
  "expo": {
    "name": "My taskly",
    "slug": "mytaskly",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "scheme": "mytaskly",
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSMicrophoneUsageDescription": "Questa app richiede l'accesso al microfono per registrare messaggi vocali.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.Gabry848Studio.Mytaskly"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.Gabry848Studio.Mytaskly",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ],
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-asset",
      "expo-av",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/icon.png"
        }
      ],
      "react-native-edge-to-edge",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#ffffff",
          "sounds": [],
          "mode": "production"
        }
      ],
      [
        "@react-native-google-signin/google-signin"
      ],
      "expo-dev-client"
    ],
    "extra": {
      "eas": {
        "projectId": "f4113004-d801-41e9-825e-4f9c5af2efd5"
      }
    },
    "owner": "gabry848"
  }
}
